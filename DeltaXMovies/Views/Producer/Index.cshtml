@model IEnumerable<DeltaXMovies.Models.Producer>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProducerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProducerBio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProducerDOB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProducerSex)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProducerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProducerBio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProducerDOB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProducerSex)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ProducerId }) |
                @Html.ActionLink("Details", "Details", new { id = item.ProducerId }) |
                @*@Html.ActionLink("Delete", "Delete", new { id=item.ProducerId })*@
            </td>
        </tr>
    }

</table>